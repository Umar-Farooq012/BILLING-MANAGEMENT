SELECT 
    DPD.PLOT_NO AS D, 
    DPD.ID AS R 
FROM 
    DGMS_PLOT_DETAILS DPD
JOIN 
    DGMS_PLOTS_BOOKING DPB ON DPB.PLOT_ID = DPD.ID 
                           AND DPB.BOOKING_STATUS = 'BOOKED' 
                           AND DPB.ORG_ID = :P173_ORGANIZATION
LEFT JOIN 
    DGMS_BILLS_SETUP DBS ON DPD.ID = DBS.PLOT_ID 
                          AND DBS.ORG_ID = :P173_ORGANIZATION 
WHERE 
    DPD.ORG_ID = :P173_ORGANIZATION
    AND (DPD.PLOT_TYPE != 'FILE' OR DPD.PLOT_TYPE IS NULL)
    AND DBS.ID IS NULL; 
