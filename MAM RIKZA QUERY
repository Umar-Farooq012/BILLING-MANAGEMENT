SELECT DISTINCT
    DPD.PLOT_NO,DPB.ORG_ID,DBR.SITE_NAME,DBS.SITE_NAME,
    DPB.CLIENT_REG_NO,DBS.PLOT_ID, DPD.CATEGORY, ROUND(DPD.MARLA, 2) MARLA, DBR.TARIFF_PER_UNIT,DBR.MAINTENANCE,DBR.WASA__CHARGES,DBR.LIVING_STATUS ,DPD.NATURE,
    (DPD.PLOT_NO || ' - ' || DPB.CLIENT_REG_NO || ' ' || DBS.PLOT_ID  ||' - '||DBR.MARLA_CATEGORY || ' - ' || DPD.NATURE||' - '||DBR.MAINTENANCE||' - '||DBR.WASA__CHARGES) as D,
   DPD.ID AS R
FROM
    DGMS_PLOT_DETAILS DPD
     JOIN DGMS_PLOTS_BOOKING DPB ON DPB.PLOT_ID = DPD.ID  AND DPB.BOOKING_STATUS = 'BOOKED'  
     AND DPB.ORG_ID = :P175_ORGANIZATION AND NVL(DPD.PLOT_TYPE,'PLOT') = 'PLOT'
    
    LEFT JOIN DGMS_BILLS_SETUP DBS ON DBS.PLOT_ID = DPB.PLOT_ID   AND DBS.SITE_NAME =DPB.ORG_ID--:P175_ORGANIZATION
    LEFT JOIN DGMS_BILL_RATES DBR ON DBR.MARLA_CATEGORY = DPD.CATEGORY
     ---JOIN 	DGMS_BILL_RATES DBR ON DBR.SITE_NAME=DPB.ORG_ID
             AND DPD.NATURE=DBR.LIVING_STATUS
             AND DBS.SITE_NAME =:P175_ORGANIZATION --DBR.SITE_NAME
             AND DBR.ACTIVE='Y'
       
             WHERE DBR.SITE_NAME=:P175_ORGANIZATION--DPB.ORG_ID
